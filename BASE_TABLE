CREATE TABLE INTERVIEW_DB.PLAYGROUND_ANKITA_GOYAL.BASE_TABLE AS
select 
CAST(DATE as date) as date, 
LOCATION, 
TOTAL_CASES, 
TOTAL_ACTIVE_CASES,
TOTAL_DEATHS, 
TOTAL_RECOVERED,
NEW_CASES,
NEW_ACTIVE_CASES,
NEW_DEATHS,
NEW_RECOVERED,
TOTAL_CASES_PER_MILLION,
GROWTH_FACTOR_OF_NEW_DEATHS,
GROWTH_FACTOR_OF_NEW_CASES,
POPULATION,
NEW_CASES_PER_MILLION,
NEW_DEATHS_PER_MILLION,
CASE_RECOVERED_RATE
from FIVETRAN_INTERVIEW_DB.GOOGLE_SHEETS.COVID_19_INDONESIA_ankita_goyal
ORDER BY 1
